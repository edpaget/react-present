        "envify": "^1.2.0",
        "react": "^0.13.2",
        "react-router": "^0.13.2"
    },
    "devDependencies": {
        "html-webpack-plugin": "^1.1.0",
        "jest-cli": "^0.2.2",
        "jsx-loader": "^0.12.2",
        "react-tools": "^0.12.2",
        "webpack-dev-server": "^1.8.2",
        "webpack": "^1.4.13"
    }
